<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<GridPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="CreateDirectoryServerController"
            prefHeight="60.0" prefWidth="300.0" vgap="8">

    <HBox GridPane.hgrow="ALWAYS" GridPane.rowIndex="1">
        <TextField fx:id="tfFileName" HBox.hgrow="ALWAYS"/>
    </HBox>

    <ButtonBar GridPane.hgrow="ALWAYS" GridPane.rowIndex="3">
        <buttons>
            <Button fx:id="btnOk">Ок</Button>
            <Button fx:id="btnСancel">Отмена</Button>
        </buttons>
    </ButtonBar>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
    </padding>

</GridPane>
